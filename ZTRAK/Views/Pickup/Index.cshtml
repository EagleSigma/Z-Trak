@{
    ViewBag.Title = "Pickups";
    Layout = "~/Views/Shared/_Layout.cshtml";


}

<div id="currentUserInfo" data-upload-list="@ViewBag.FileNames" data-userphone="@ViewBag.UserCell" data-usercellprovider="@ViewBag.UserCellProvider" data-username="@ViewBag.UserName" data-userrole="@ViewBag.UserRole" data-email="@ViewBag.UserEmail" data-fullname="@ViewBag.FullName" data-code="@ViewBag.Code" data-codeexp="@ViewBag.CodeExp" data-pincode="@ViewBag.PinNumber" data-teamname="@ViewBag.TeamName"></div>

<ul id="pickupsContainer" data-bind="template: {foreach: pickupCollection}" class="list-group noBulletsList">

    @*Show Each Pickup using the correct template*@

    <li data-bind="template: { name: Mode, data: $data, afterRender: doneRenderingItem }" style="margin-bottom:20px">

    </li>

</ul>

@* #region Templates *@

<script type="text/html" id="display">

    <div class="panel panel-primary" style=" border: 3px solid gray;">
        <div class="cali">

            @*Pickup Number and Date Created*@
            <div class="row">
                <div>
                    <span data-bind="attr: {id: 'pu' + PickupNumber() }"></span>
                    <div class="col-xs-3"> <span class="pickup-item-headers display-pickups cali2" data-bind="text: PickupNumber" data-toggle="tooltip" title="Pickup Number" /> </div>
                    <div class="col-xs-9"> <span class="pickup-item-headers pull-right date-field cali2" data-bind="text: Created" data-toggle="tooltip" title="Date Pickup Was Created" /> </div>
                </div>
            </div>
        </div>

        <div class="panel-body" style="padding: 0;">

            @* PICKUP INFO*@
            <div class="container-fluid">

                @*Location*@
                <div class="row">
                    <div class="col-xs-12" style="padding-left: 0; padding-right: 0">
                        <table id="table1" class=" location-name-background">
                            <tr>
                                <th class="field-label">
                                    <span>START:</span>
                                </th>
                            </tr>
                            <tr>
                                <td>
                                    <span class="location-name-title" data-toggle="tooltip" title="Location Name" data-bind="text: LocationName"></span>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>

                @*Destination*@
                <div class="row" style="border-top: 3px solid gray; border-bottom: 3px solid gray;">
                    <div class="col-xs-12" style="padding-left: 0; padding-right: 0;">

                        <table id="table1" class="destination-name-background">
                            <tr>
                                <th class="field-label">
                                    <span>END:</span>
                                </th>
                            </tr>
                            <tr>
                                <td>
                                    <span class="destination-name-title" data-toggle="tooltip" title="Destination Name" data-bind="text: Destination"></span>
                                </td>
                            </tr>
                        </table>

                    </div>
                </div>

                @*Team Name, Sales Rep and Status *@
                <div class="row" style="padding-left: 2px; padding-right: 2px; padding-top: 3px;">
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; ">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>TEAM NAME:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-bind="text: Team" data-toggle="tooltip" title="Team Name"> </span>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; text-align: center;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>SALES REP:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-bind="text: Rep" data-toggle="tooltip" title="Sales Rep Assigned To This Pickup"></span>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; text-align: right;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>STATUS:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-toggle="tooltip" title="Pickup Status" data-bind="text: Status"></span>
                                </td>
                            </tr>
                        </table>

                    </div>
                </div>

                @* Surgical Coordinator, Courier and Created By*@
                <div class="row" style="padding-left: 2px; padding-right:2px; padding-top: 3px; background-color: #f5f5f5; border-top: 2px solid darkGray">

                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; text-align: center;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>SC:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-bind="text: Manager" data-toggle="tooltip" title="Surgical Coordinator"> </span>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>COURIER:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-toggle="tooltip" title="Courier Assigned To This Pickup" data-bind="text: TransporterName"></span>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; text-align: right">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>CREATED BY:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-bind="text: CreatedBy" data-toggle="tooltip" title="User That Created This Pickup"> </span>
                                </td>
                            </tr>
                        </table>

                    </div>

                </div>

                @*Last Modified By, Modified Time  *@
                <div class="row" style="padding-left: 2px; padding-right: 2px; padding-top: 3px; border-top: 2px solid darkGray">
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>LAST EDIT BY:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2" data-bind="text: LastEditBy" data-toggle="tooltip" title="Last User That Modified This Pickup"></span>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div class="col-xs-4 col-xs-offset-4" style="padding-left: 3px; padding-right: 3px; text-align:right;">

                        <table id="table1">
                            <tr class="field-label">
                                <th class="field-header">
                                    <span>LAST MODIFIED:</span>
                                </th>
                            </tr>
                            <tr>
                                <td style="padding-bottom: 3px;">
                                    <span class="pickup-item-headers-2 date-field" data-bind="text: LastModified" data-toggle="tooltip" title="Last Time This Pickup Was Modified"> </span>
                                </td>
                            </tr>
                        </table>

                    </div>
                </div>

                @*Notes*@
                <div class="row" style="padding-left: 2px; padding-right: 2px; background-color: #ffffff; border-top: 2px solid darkGray; border-bottom: 2px solid darkGray; ">
                    <div class="col-xs-12" style="padding-left: 3px; padding-right: 3px;">
                        <span class="notesLabel"> NOTES:</span>   <span class="pickup-item-headers-2" data-bind="text: Notes" data-toggle="tooltip" title="Team Name"> </span>
                    </div>
                </div>

                @*ITEMS Container*@
                <ul id="itemsContainer" data-bind="foreach: Items" class="itemHolderList">

                    <li>
                        @* QTY, DESCRIPTION, ADDED BY, DATE*@
                        <div data-bind=" attr: {id: 'item'+ ID}">
                            <div class="row" data-bind="css: { PhotoAvailable: Picture1() != 'none', PhotoNotAvailable: Picture1() === 'none'}" style="padding-left: 0; padding-right: 0; display: block; border-top: 1px solid black; border-bottom: 1px solid black; ">
                                @*ADD BY*@
                                <div class="col-xs-12" style="padding-left: 3px; padding-right: 3px;">

                                    <table id="table1">
                                        <tr>
                                            <td style="text-align:center; max-width:100%; width:1em; overflow:visible;">
                                                <a class="zfm-display-photo">
                                                    <i data-bind="style: { display: Picture1() === 'none' ? 'none' : 'block' }" style="margin-right: 7px; font-size: 2em; line-height: 1.3em; padding-top:6px; " class="fa fa-lg fa-fw fa-camera"> </i>
                                                </a>
                                            </td>
                                            <td style=" text-align: left; max-width:200px;">
                                                <span> <button data-bind=" attr: {title:Picture1()}" class="zfm-display-photo"><span class="pickup-item-list" data-bind="text: AddedBy"></span>  </button></span>
                                            </td>
                                            <td style=" text-align: right;">
                                                @*ADD DATE*@
                                                <span class="date-field pickup-item-list" style="text-align: right; padding-top:4px; display: block;" data-bind="text: AddedDate"> </span>
                                            </td>

                                        </tr>
                                    </table>

                                </div>

                            </div>
                        </div>
                    </li>
                </ul>
                @*Containder end*@

            </div>

        </div>

        <div class="panel-footer" data-bind="attr: {id: 'alert' + PickupNumber()}">


            <div class=" btn-group">
                <button class="btn btn-default zfm-edit" data-toggle="tooltip" title="Edit Pickup"><i class="fa fa-fw fa-pencil"></i></button>
                <button class="btn btn-default zfm-photo" data-toggle="tooltip" title="Upload Picture"><i class="fa fa-fw fa-camera"></i></button>
                <button class="btn btn-default btn-danger zfm-delete" data-toggle="tooltip" title="Delete Pickup"><i class="fa fa-fw fa-trash-o"></i></button>
            </div>

        </div>
    </div>

</script>

