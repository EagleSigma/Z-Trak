<script type="text/html" id="edit">

    <div class="panel panel-primary">

        <div data-bind="attr: {id: ID, 'data-owner': CreatedBy, 'data-team': Team, 'data-status': Status }" class="cali">

            @*Pickup Number and Date Created*@
            <div class="row">
                <div>
                    <span data-bind="attr: {id: 'pu' + PickupNumber() }"></span>
                    <div class="col-xs-3"> <span class="pickup-item-headers cali2" data-bind="text: PickupNumber" /> </div>
                    <div class="col-xs-9"> <span class="pickup-item-headers pull-right date-field cali2" data-bind="text: Created" /> </div>
                </div>
            </div>
        </div>


        <div class="panel-body" style="padding: 0;">

            @* PICKUP INFO*@
            <div class="container-fluid">

                @*Location*@
                <div class="row">

                    <div class="col-xs-12" style="padding-left: 0; padding-right: 0">
                        <span class="location-name-background ">
                            <input style="max-width:100%; margin: 0; padding: 0;" type="text" data-provide="typeahead" class="loc-typeahead pickup-item-loc-name zfm-input inputEdit ios" autocomplete="off" placeholder="Location Name" data-bind="value: LocationName" />
                        </span>
                    </div>
                </div>

                @*Destination*@
                <div class="row" style="border-top: 2px solid darkGray">

                    <div class="col-xs-12" style="padding-left: 0; padding-right: 0">
                        <span class="destination-name-background ">
                            <input style="max-width:100%; margin: 0; padding: 0;" type="text" data-provide="typeahead" class="des-typeahead pickup-item-loc-name zfm-input inputEdit ios" autocomplete="off" placeholder="Destination Name" data-bind="value: Destination" />
                        </span>
                    </div>
                </div>

                @*Team Name, Sales Rep, Status*@
                <div class="row" style="padding-left: 2px; padding-right: 2px; background-color: #f5f5f5;">
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px; ">
                        <input type="text" class="teams-typeahead zfm-input2 pickup-item-headers-2 ios" placeholder="Team Name" data-toggle="tooltip" title="Team Name" data-bind="value: Team" />
                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">
                        <input data-provide="typeahead" class=" zfm-input2 pickup-item-headers-2 rep-typeahead ios" data-bind="value: Rep" data-toggle="tooltip" title="Sales Rep Assigned To This Pickup" placeholder="Sales Rep" />
                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">
                        <input class="status-typeahead zfm-input2 pickup-item-headers-2 ios" data-bind="value: Status" data-toggle="tooltip" title="Pickup Status" placeholder="Pickup Status" />
                    </div>
                </div>

                @*Surgical Coordinator, Courier and Created By*@
                <div class="row" style="padding-left: 2px; padding-right:2px">

                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">
                        <input class="warehouse-typeahead zfm-input2 pickup-item-headers-2 ios" data-bind="value: Manager" data-toggle="tooltip" title="Surgical Coordinator" placeholder="Surgical Coordinator" />
                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">
                        <input type="text" class="courier-typeahead zfm-input2 pickup-item-headers-2 ios" placeholder="Courier" data-toggle="tooltip" title="Courier Assigned To This Pickup" data-bind="value: TransporterName" />
                    </div>
                    <div class="col-xs-4" style="padding-left: 3px; padding-right: 3px;">
                        <span class="pickup-item-headers-2" data-bind="text: CreatedBy" data-toggle="tooltip" title="User That Created This Pickup"> </span>
                    </div>
                </div>

