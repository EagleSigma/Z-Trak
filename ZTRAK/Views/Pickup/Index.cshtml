                                <input id="address-gps" style="max-width: 100%; text-align: left; margin: 0; padding: 0; padding-left: 5px;font-weight: normal; font-family: robotoregular" type="text" class="zfm-input inputEdit ios" placeholder="Address" />
                            </span>
                        </div>
                    </div>

                    @*City, State and State*@
                    <div class="row" style="padding-left: 5px; padding-right:5px">

                        <div class="col-xs-7" style="padding-left: 0; padding-right:3px">
                            <input id="city-gps" class=" zfm-input2 pickup-item-headers-2 ios" data-toggle="tooltip" title="City" placeholder="City" />
                        </div>
                        <div class="col-xs-3" style="padding-left: 0; padding-right:3px">
                            <input id="state-gps" class="zfm-input2 pickup-item-headers-2 ios" data-toggle="tooltip" title="State" placeholder="State" />
                        </div>
                        <div class="col-xs-2" style="padding-left: 0; padding-right:0">
                            <input id="zip-gps" class="zfm-input2 pickup-item-headers-2 ios" data-toggle="tooltip" title="Zip" placeholder="Zip" />
                        </div>
                    </div>

                    @*MAP*@
                    <div class="row" style="padding-left: 2px; padding-right: 2px; padding-top: 5px; background-color: #f5f5f5; border-top: 2px solid darkGray; border-bottom: 5px solid #f5f5f5; ">
                        <div id="myMap" style="background-color: #85ecbf; height: 300px; position:relative;"></div>
                        <div id="output"></div>
                    </div>

                </div>

            </div>
            <div class="modal-footer" style="padding: 0; padding-bottom: 5px; padding-right: 10px;">
                <button type="button" data-toggle="tooltip" title="GET GPS FOR CURRENT LOCATION" class="btn btn-info find-gps"> <i class="fa fa-fw fa-search"></i> </button>
                <button type="button" data-toggle="tooltip" title="SAVE" class="btn  btn-success save-gps"> <i class="fa fa-fw fa-save"></i> </button>
                <button type="button" data-toggle="tooltip" title="DISMISS" class="btn btn-danger dismiss-gps" data-dismiss="modal"> <i class="fa fa-fw fa-level-down"></i> </button>
            </div>

        </div> @*modal-content*@
    </div> @*modal-dialog*@
</div>

